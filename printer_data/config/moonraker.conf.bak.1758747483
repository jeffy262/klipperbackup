[file_manager]
enable_object_processing: True

[server]
host: 0.0.0.0
port: 7125
klippy_uds_address: /home/biqu/printer_data/comms/klippy.sock

# ---- KEEP ONLY THIS ONE BLOCK ----
[authorization]
trusted_clients:
  127.0.0.1
  192.168.0.0/16
  10.0.0.0/8
  127.0.0.0/8
  169.254.0.0/16
  172.16.0.0/12
  FC00::/7
  FE80::/10
  ::1/128
cors_domains:
  *.lan
  *.local
  *://localhost
  *://localhost:*
  *://my.mainsail.xyz
  *://app.fluidd.xyz
